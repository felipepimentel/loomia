  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/app-desktop/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/workspace:run-commands",
      "options": {
