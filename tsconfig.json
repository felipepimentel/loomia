{
  "compilerOptions": {
      "jsx": "react",
      "target": "ESNext",
      "module": "commonjs",
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "noImplicitAny": true,
      "sourceMap": true,
      "strict": true,
      "outDir": "dist",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "baseUrl": "./",
      "paths": {
          "@/*": ["src/*"],
      }
  }
}
